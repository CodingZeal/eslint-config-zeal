{
  "name": "eslint-config-zeal",
  "description": "ESLint sharable configuration matching Zeal's style guide",
  "version": "0.13.0",
  "main": "index.js",
  "scripts": {
    "missing-rules": "run-p missing-rules:*",
    "missing-rules:core": "eslint-find-rules --unused",
    "missing-rules:react": "eslint-find-rules --no-core --unused react.js",
    "missing-rules:react-native": "eslint-find-rules --no-core --unused react-native.js",
    "test": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodingZeal/eslint-config-zeal.git"
  },
  "homepage": "https://github.com/CodingZeal/eslint-config-zeal",
  "bugs": {
    "url": "https://github.com/CodingZeal/eslint-config-zeal/issues"
  },
  "author": "Randy Coulman <randy@codingzeal.com> (http://codingzeal.com)",
  "license": "MIT",
  "keywords": [
    "check",
    "checker",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "eslint",
    "eslint-config",
    "eslintconfig",
    "lint",
    "quality",
    "style",
    "style checker",
    "style linter"
  ],
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.11.1",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.6.0",
    "eslint-plugin-react-native": "^2.2.0",
    "npm-run-all": "^3.1.2"
  },
  "peerDependencies": {
    "babel-eslint": "^7.1.0",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0"
  }
}
